                    //Remove the starting \n that might exist
                    var textNode =3D pre.firstChild;
                    if (textNode && textNode.nodeType =3D=3D 3 && =
textNode.nodeValue.startsWith("\n")) {
                        var newText =3D =
doc.createTextNode(textNode.nodeValue.trim());
                        textNode.parentNode.replaceChild(newText, =
textNode);
