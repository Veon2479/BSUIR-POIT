function CCE_GetChildElementByID(elem, id) {
    var childCount =3D elem.childNodes.length;
    for (var i =3D 0; i < childCount; i++) {
        var childElem =3D elem.childNodes[i];
        if (childElem.id =3D=3D id) {
            return childElem;
